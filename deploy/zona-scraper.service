[Unit]
Description=Zona Prop Scraper API
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/zona-prop-scraper
Environment="PATH=/var/www/zona-prop-scraper/venv/bin"
ExecStart=/var/www/zona-prop-scraper/venv/bin/gunicorn --config gunicorn_config.py wsgi:app

Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
